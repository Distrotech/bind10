{
    "version": 3,
    "Logging": {
        "loggers": [ {
            "severity": "DEBUG",
            "name": "*",
            "debuglevel": 99
        } ]
    },
    "Resolver": {
        "query_acl": [ {
            "action": "REJECT",
            "from": "127.0.0.1"
        } ],
        "listen_on": [ {
            "port": 56176,
            "address": "127.0.0.1"
        } ]
    },
    "Init": {
        "components": {
            "bundy-resolver": {
                "kind": "needed"
            },
            "bundy-cmdctl": {
                "kind": "needed",
                "special": "cmdctl"
            }
        }
    }
}
