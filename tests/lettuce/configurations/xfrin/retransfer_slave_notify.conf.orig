{
    "version": 3,
    "Logging": {
        "loggers": [ {
            "debuglevel": 99,
            "severity": "DEBUG",
            "name": "*"
        } ]
    },
    "Auth": {
        "listen_on": [ {
            "address": "::1",
            "port": 56176
        } ]
    },
    "data_sources": {
        "classes": {
            "IN": [{
                "type": "sqlite3",
                "params": {
                    "database_file": "data/xfrin-notify.sqlite3"
                }
            }]
        }
    },
    "Xfrin": {
        "zones": [ {
            "name": "example.org",
            "master_addr": "::1",
            "master_port": 56177,
            "request_ixfr": "no"
        } ]
    },
    "Zonemgr": {
        "secondary_zones": [ {
            "name": "example.org",
            "class": "IN"
        } ]
    },
    "Init": {
        "components": {
            "bundy-auth": { "kind": "needed", "special": "auth" },
            "bundy-xfrin": { "address": "Xfrin", "kind": "dispensable" },
            "bundy-zonemgr": { "address": "Zonemgr", "kind": "dispensable" },
            "bundy-stats": { "address": "Stats", "kind": "dispensable" },
            "bundy-cmdctl": { "special": "cmdctl", "kind": "needed" }
        }
    }
}
